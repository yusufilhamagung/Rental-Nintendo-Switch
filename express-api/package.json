{
  "name": "express-api",
  "version": "1.0.0",
  "private": true,
  "description": "API Sewa Nintendo Switch (TypeScript, Clean Architecture)",
  "author": "Ucupcupcup",
  "license": "ISC",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "build": "tsc -p .",
    "start": "node -r module-alias/register dist/server.js"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.22.1",
    "mysql2": "^3.11.3",
    "module-alias": "^2.2.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.9.3",
    "tsconfig-paths": "^4.2.0"
  },
  "_moduleAliases": {
    "@domain": "dist/domain",
    "@application": "dist/application",
    "@infrastructure": "dist/infrastructure",
    "@interfaces": "dist/interfaces",
    "@config": "dist/config",
    "@shared": "dist/shared"
  }
}
